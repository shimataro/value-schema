  "author": "shimataro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shimataro/node-adjuster.git"
  },
  "bugs": {
    "url": "https://github.com/shimataro/node-adjuster/issues"
  },
  "homepage": "https://github.com/shimataro/node-adjuster#readme",
  "keywords": [
    "param",
